<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE note [
<!ENTITY PROD "P15ik">
<!ENTITY LIBMACROS "/star/u/kabir/GIT/BrightSTAR/runMacros/lib/libRunMacros.so">
<!ENTITY MACRO "/star/u/kabir/GIT/BrightSTAR/jobMacro.C">
<!ENTITY TRIG "production_pp200trans_2015||production_fms_pp200trans_2015">
<!ENTITY OUTNAME "RunJetFinder">
<!ENTITY LIBBRIGHT "/star/u/kabir/GIT/BrightSTAR/libBrStar/lib/libBrStar.so">
<!ENTITY FILEN "~st_fms_16">
<!ENTITY ROOTSETUP "/star/u/kabir/GIT/BrightSTAR/rootlogon.C">
<!ENTITY SOURCE "/star/u/kabir/GIT/BrightSTAR/.sl73_gcc485">
<!ENTITY BASEDIR "/star/u/kabir/GIT/BrightSTAR">
<!ENTITY OUTDIR "jobResults/16072006">
<!ENTITY RUN "16072006">
<!ENTITY SETUPFILE "/star/u/kabir/GIT/BrightSTAR/setup.csh">
<!ENTITY FILET "daq_reco_mudst">
<!ENTITY LIBANA "/star/u/kabir/GIT/BrightSTAR/analysis/lib/libStAnalysis.so">
<!ENTITY JOBOUTDIR "jobOutput">
]>
<!-- Created on Tue Oct 15 17:04:39 2019 -->
<job inputOrder="runnumber" splitBy="runnumber" maxFilesPerProcess="1" filesPerHour="3">

	<Generator>
		<Location> ./jobOutput </Location>
	</Generator>

	<command>
		stardev

		set THISRUN = `echo $INPUTFILE0 | sed 's|.*_\([0-9]*\)_.*_\([0-9]*\).MuDst.root|\1_\2|'`
		set OUTFILE = &OUTNAME;_${THISRUN}.root
		set OUTDIR  = &OUTDIR;
		source &SETUPFILE;
		root4star -b -q &MACRO;\(\"$FILELIST\",\"$OUTFILE\"\)

		mv *.root $SUBMITTINGDIRECTORY/$OUTDIR/;
	</command>

	<stdout URL="file:&JOBOUTDIR;/log_&OUTNAME;/&RUN;/$FILEBASENAME.out" />
	<stderr URL="file:&JOBOUTDIR;/log_&OUTNAME;/&RUN;/$FILEBASENAME.err" />
	
	<SandBox>
	<Package>
		<File>file:&MACRO;</File>
		<File>file:&SOURCE;</File>
		<File>file:&SETUPFILE;</File>
		<File>file:&ROOTSETUP;</File>
		<File>file:&LIBBRIGHT;</File>
		<File>file:&LIBANA;</File>
		<File>file:&LIBMACROS;</File>
	</Package>
	</SandBox>

	<input URL="catalog:star.bnl.gov?runnumber=&RUN;,filename&FILEN;,filetype=&FILET;,production=&PROD;,storage!~HPSS,trgsetupname=&TRIG;" nFiles="all" />
	<!-- <input URL="catalog:star.bnl.gov?runnumber=&RUN;,filename&FILEN;,filetype=&FILET;,production=&PROD;,storage!~HPSS,trgsetupname=&TRIG;" nFiles="1" /> -->
</job>
