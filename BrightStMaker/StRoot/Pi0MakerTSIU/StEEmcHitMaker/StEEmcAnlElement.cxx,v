@/*!
 *
 * \class StEEmcAnlElement_t
 * \author Stephen Gliske <sgliske@@anl.gov>
 *
 * See description in header file.
 *
 */

#include "StEEmcAnlElement.h"
#include "StRoot/St_base/StMessMgr.h"

// static member function to clear set
void StEEmcAnlElement_t::scaleSet( StEEmcAnlElementSet_t &elemSet, Float_t w ){
   for( StEEmcAnlElementSet_t::iterator elementIter = elemSet.begin(); elementIter != elemSet.end(); ++elementIter )
      const_cast< StEEmcAnlElement_t* >( &(*elementIter) )->mEnergy *= w;
};


ClassImp( StEEmcAnlElement_t );

