@/*!
 *
 * \class AnlEMLineParams_t
 * \author Stephen Gliske <sgliske@@anl.gov>
 *
 * A structure used in the StEEmcHitMakerAnlEM_t class to keep track of line parameters and a few other things.
 *
 */

#ifndef _EEMC_ANL_LINE_PARAMS_H_
#define _EEMC_ANL_LINE_PARAMS_H_

#include <vector>

class AnlEMLineParams_t;

typedef std::vector< AnlEMLineParams_t > AnlEMLineParamsVec_t;

// class for points
class AnlEMLineParams_t {
 private:
   Short_t mSector;
   Float_t mA, mB, mW;

 public:
   AnlEMLineParams_t( Short_t sector, Float_t w, Float_t a, Float_t b ) : mSector(sector), mA(a), mB(b), mW(w) { /* */ };

   Short_t& sector(){ return mSector; };
   Float_t& a(){ return mA; };
   Float_t& b(){ return mB; };
   Float_t& weight(){ return mW; };
};

#endif

